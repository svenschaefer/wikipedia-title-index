{
  "name": "wikipedia-title-index",
  "version": "1.2.2",
  "description": "Local Wikipedia title index build and constrained query service.",
  "license": "MIT",
  "type": "commonjs",
  "main": "src/index.js",
  "types": "types/index.d.ts",
  "bin": {
    "wikipedia-title-index": "bin/wikipedia-title-index.js"
  },
  "exports": {
    ".": "./src/index.js",
    "./openapi.yaml": "./openapi/openapi.yaml"
  },
  "files": [
    "bin/",
    "src/",
    "docs/",
    "types/",
    "openapi/",
    "README.md",
    "LICENSE",
    "SECURITY.md"
  ],
  "engines": {
    "node": ">=24.10.0"
  },
  "scripts": {
    "test": "node --test test/*.test.js",
    "lint:openapi": "node scripts/lint-openapi.js",
    "release-check": "node scripts/ensure-clean-worktree.js && npm test && npm run lint:openapi && npm pack --dry-run"
  },
  "devDependencies": {
    "@redocly/cli": "^1.34.5"
  }
}
